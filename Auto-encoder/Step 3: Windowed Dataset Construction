ds = GenotypeWindowDataset()
print("Total windows:", len(ds))

# Inspect one window
w = ds[0]
print("Window shape:", w.shape)        # should be (3, 1000)
print("Sample values (3×5):\n", w[:3, :5])
PYCODE
